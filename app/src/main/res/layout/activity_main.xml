<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="@android:dimen/app_icon_size"
    tools:context=".MainActivity">

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView5"
        android:layout_width="237dp"
        android:layout_height="88dp"
        android:layout_margin="@android:dimen/app_icon_size"
        android:padding="@android:dimen/dialog_min_width_major"
        android:paddingLeft="@android:dimen/app_icon_size"
        android:paddingTop="@android:dimen/dialog_min_width_major"
        android:paddingRight="@android:dimen/dialog_min_width_minor"
        android:paddingBottom="@android:dimen/app_icon_size"
        android:text=" Buscar Receta"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#1E1C1D"
        android:textColorHighlight="#E91E63"
        android:textColorHint="#E91E63"
        android:textColorLink="#E91E63"
        android:visibility="visible"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="16dp"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="36dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@+id/autoCompleteTextView5"
        app:srcCompat="@android:drawable/ic_menu_search"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/button3"
        android:layout_width="101dp"
        android:layout_height="36dp"
        android:layout_marginTop="52dp"
        android:text="Perfil"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="625dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="105dp">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_margin="@android:dimen/dialog_min_width_major"
            android:foreground="@drawable/categoias"
            android:text="CATEGORÃAS"
            android:textColor="#131312" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="116dp"
            android:foreground="@drawable/platos_fuertes"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="113dp"
            android:foreground="@drawable/postres"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="134dp"
            android:foreground="@drawable/acompanamientos"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="113dp"
            android:foreground="@drawable/bebidas"
            android:text="TextView" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>